{
	"manifest_version": 2,

	"name": "apostrophe",
	"description": "link search navigation for Chrome",
	"version": "0.7",
	
	"browser_action": {
		"default_icon": "apostrophe.png",
		"default_popup": "popup.html"
	},

	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*", "*://*/*"],
			"js": ["searchScript.js"],
			"css": ["apostrophe.css"],
			"all_frames": true
		}
	],


	"permissions": [
		"activeTab"
	]
	
}
